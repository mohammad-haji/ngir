  "scripts": {
    "ng": "ng",
    "start": "node dist/server",
    "startdev": "ng serve",
    "build": "ng build --prod --aot --extract-css",
    "lint": "ng lint ngir",
    "build:client-and-server-bundles": "ng build --prod && ng run ngir:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "local-versioning": "node ./src/version.js",
    "run-conventional-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "version": "npm run local-versioning && npm run run-conventional-changelog"
  },
